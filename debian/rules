#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.6.0+git at
# Mon, 19 Nov 2012 18:22:48 +0100

%:
	dh $@ --with python3

override_dh_auto_configure:
	if [ ! -f cdist/version.py ]; then $(MAKE) version; fi

override_dh_auto_install:
	#python3 setup.py install --root=debian/cdist --install-layout=deb --install-lib=/usr/share/cdist --install-scripts=/usr/share/cdist
	python3 setup.py install --root=debian/cdist --install-layout=deb
	find debian/cdist -name .*.swp -exec rm '{}' +
	find debian/cdist -name .gitignore -exec rm '{}' +

override_dh_install_man:
	dh_installman `find docs/man/ -name \*.1 -o -name \*.7 > debian/manpages`

override_dh_clean:
	rm -rf build/
	dh_clean

override_dh_auto_build:
